{
  "elastic": {
    "host": "TESTSJC20-ES01.revsw.net:9200",
    "version": "1.5"
  },
  "proxies": {
    "production": "http://lga02-bp01.revsw.net",
    "testing": "http://lga02-bp02.revsw.net"
  },
  "defaults": {
    "topAgents": 50,
    "topReferers": 100,
    "topURLs": 100,
    "minCount": 200,            //  least amount of hits for the any combination of the url, port, method, agent and referer
    "tooBigContent": 1048576    //  1M
  },
  "log_config": {
    "transports": {
      "Console": {
        "level": "info",
        "colorize": true,
        "prettyPrint": true,
        "humanReadableUnhandledException": true,
        "showLevel": true
      },
      "File": {
        "filename": "./log/revsw-proxy-tester.log",
        "level": "info",
        "prettyPrint": true
      },
      "Rsyslog": {
        "level": "error",
        "app-name": "revsw-proxy-tester"
      }
    }
  },
  "comparators": {
    "default": {
      "content-type": {},
      "content-length": {},
      "etag": {
        "comparator": "return ( prod === test || ( prod.substr( 0, 2 ) === 'W/' && test.substr( 0, 2 ) === 'W/' ) )"
      },
      "last-modified": {},
      "x-rev-cache": {}
    },
    // "example.com": {
    //   "content-length": false, // do not check
    //   "etag": {
    //     "comparator": "return ( prod === test || ( prod.substr( 0, 2 ) === 'W/' && test.substr( 0, 2 ) === 'W/' ) )"
    //   },
    // },
    "static.treato.com": {
      "etag": {
        "comparator": "return ( prod === test || prod.substr( 6 ) === test.substr( 6 ) );"
        // "etag": "\"88f94-32183-5204253e4fec0\"",
        // "etag": "\"ebad6-32183-5204253e4fec0\"",
      }
    },
    "s1.mcstatic.com": {
      "last-modified": {
        //  'Wed, 16 Sep 2015 12:40:52 GMT' and 'Wed, 16 Sep 2015 12:40:54 GMT'
        //  difference less then 5 seconds is ok
        "comparator": "return ( ( new Date( prod ) ).getTime() - ( new Date( test ) ).getTime() < 5000 );"
      }
    }
  }
}

